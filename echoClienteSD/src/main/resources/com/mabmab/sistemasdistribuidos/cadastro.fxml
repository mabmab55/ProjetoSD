<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" minHeight="400" prefHeight="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mabmab.sistemasdistribuidos.ControllerCadastro">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Button fx:id="configurarIP" mnemonicParsing="false" onAction="#clickConfigurarIP" text="Configurar IP" />
    <Label text="CADASTRO">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <TextField fx:id="nomeCadastro" layoutX="30.0" layoutY="115.0" promptText="nome" />
    <TextField fx:id="emailCadastro" promptText="email" />
    <TextField fx:id="senhaCadastro" promptText="senha" />
    <Button fx:id="enviarCadastro" onAction="#onSendClick" text="Enviar" />
    <Button fx:id="irLogin" mnemonicParsing="false" onAction="#irParaLogin" text="Ir para Login" />
</VBox>
