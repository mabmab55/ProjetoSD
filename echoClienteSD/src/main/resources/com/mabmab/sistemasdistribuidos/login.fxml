<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="192.0" prefWidth="190.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mabmab.sistemasdistribuidos.ControllerLogin">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label text="LOGIN">
        <font>
            <Font size="18.0" />
        </font>
    </Label>
    <TextField fx:id="emailLogin" promptText="email" />
    <TextField fx:id="senhaLogin" promptText="senha" />
    <Button fx:id="enviarLogin" onAction="#onSendClick" text="Enviar" />
   <Button fx:id="irCadastro" mnemonicParsing="false" onAction="#irParaCadastro" text="Ir Para Cadastro" />
</VBox>
